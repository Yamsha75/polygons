<def name="Polygon colshape editor polugin">
    <element name="vertex" friendlyname="Vertex">
        <data name="position" type="coord3d" required="true" default="0,0,0" />
        <data name="next" type="element:vertex" required="false" default="" />
        <marker editorOnly="true" type="checkpoint" size="0.25" color="#000000ee" />
    </element>
    
    <element name="polygon" friendlyname="Polygon">
        <data name="position" type="coord3d" required="true" default="0,0,0" />
        <data name="first" type="element:vertex" required="false" default="" />
        <marker editorOnly="true" type="checkpoint" size="1" color="#000000ee" />
    </element>

